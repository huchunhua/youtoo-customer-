.presentation{
    width:345px;
    height:62px;
    background:rgba(255,255,255,1);
    box-shadow:0px 2px 10px 0px rgba(232,233,235,0.3);
    margin:10rpx 10rpx;
    padding:30rpx 20rpx;
}
.cf::after, .cf::before {
  content: " ";
  display: table;
}
.cf::after {
  clear: both;
}
.fr {
  float: right;
}
.text-red{
    color:#ff6a6a;
}
.text-grey{
    color:#959595;
}
.margin{
    margin-top: 30rpx;
}
.presentation{
    font-size: 24rpx;
}
.name{
    font-size: 30rpx;
}
.num{
    margin-left:60rpx;
}