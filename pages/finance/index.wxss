@import "/icon.wxss";
@import "/base-rpx.wxss";
Page {
    background:rgba(250,250,250,1);
    text-align: center;
}
.bg-white{
    background-color: #fff;
}
.bg-yellow{
    background:linear-gradient(134deg,rgba(248,228,200,1) 0%,rgba(244,208,139,1) 100%);
    border-radius:0px 0px 4px 4px;
    color: #724E00!important;
}
.total{
    width: 100%;
    height: 328rpx;
    background-image: url(https://youbao-1251769479.cos.ap-chengdu.myqcloud.com/youtu_sale/bg_caiwu_wps.png);
    background-size: 100%;
    background-position: center;
    background-repeat: no-repeat;
    color:#724E00;
}
.border-right{
    border-right: 1px solid #f4f4f4;
}
.today_check{
    font-size: 64rpx;
    font-weight: 700;
}
.cashing .more-icon{
    margin-top: 15rpx;
    width: 40rpx;
    height: 40rpx;
}
.ver_title{
    height: 120rpx;
    line-height: 120rpx;
}
.cash_wrap{
    position: relative;
    top:-20rpx;
    width:calc(100% - 60rpx);
    left:30rpx;
    border-radius: 8rpx;
}
.cash_wrap image{
    width: 80rpx;
    height: 80rpx;
}
.btn-cash{
    margin:12rpx auto;
    width: 176rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border-radius: 60rpx;
    color:#fff;
}
.cashing{
    line-height: 70rpx;
}
.more-icon{
    width: 10rpx;
    height: 22rpx;
}
.write-item{
    background:rgba(255,255,255,1);
    box-shadow:0px 2px 10px 0px rgba(232,233,235,0.3);
}